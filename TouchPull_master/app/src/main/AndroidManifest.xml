<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cj.android.touchpull_master">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:name=".ListViewActivity" />
        <activity android:name=".ScrollViewActivity" />
        <activity android:name=".GridViewActivity" />
        <activity android:name=".WebViewActivity"/>
        <activity android:name=".TextViewActivity" />
        <activity android:name=".EditTextActivity"/>
        <activity android:name=".ExpandableListViewActivity"/>
        <activity android:name=".ViewPagerActivity"/>
        <activity android:name=".CJ.CJActivity"/>
    </application>

</manifest>
